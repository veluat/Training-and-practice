            stop: function() {
                this.started = false;
            },

            drive: function() {
                if (this.started) {
                    alert("Zoom zoom!");
                } else {
                    alert("You need to start the engine first.");
                }
            }
        };

cadi.start();
cadi.drive();
cadi.stop();
taxi.start();
taxi.drive();
taxi.stop();
fiat.start();
fiat.drive();
